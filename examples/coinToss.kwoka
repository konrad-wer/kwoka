

effect Toss
{
  Coin() :: Bool
}

fn sublist(xs)
{
  case xs of
    [] => []
    x : xs =>
      if Coin() then
        x : sublist(xs)
      else
        sublist(xs)
}

/*
  Generating all sublists of the given list.
*/
fn main()
{
  handle<Toss>(sublist([1,2,3,4,5]))
  {
    return(x) => [x],
    Coin() => resume(True) @ resume(False)
  }
}